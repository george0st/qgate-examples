# run cass.env
python3.11 perf_cql/perf_cql.py run -e 0-cass.env -d perf_cql

# run cass-hgh.env
python3.11 perf_cql/perf_cql.py run -e 0-cass-perf.env -d perf_cql

# run cass.env & cass-hgh.env without terminal
nohup python3.11 perf_cql/perf_cql.py run -e 0-cass.env -d perf_cql
nohup python3.11 perf_cql/perf_cql.py run -e 0-cass-hgh.env -d perf_cql